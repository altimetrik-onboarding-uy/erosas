    <validationRules>
        <fullName>Owners_manager_can_chante_to_ready</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
    (ISCHANGED(Status__c)),
   ($User.Id &lt;&gt; Owner:User.ManagerId ),
   ISPICKVAL(Status__c ,&quot;Ready&quot;)
)</errorConditionFormula>
        <errorMessage>Only the ownerâ€™s manager can change the status to Ready.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Post_created_with_draft_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT(ISPICKVAL(Status__c ,&quot;Draft&quot;)),
    ISNEW())</errorConditionFormula>
        <errorMessage>Post can only be created with Draft status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  
   (ISCHANGED(Status__c)),
   (NOT(ISPICKVAL(Status__c ,&quot;Under Review&quot;))),
   (NOT(ISPICKVAL(Status__c ,&quot;Ready&quot;)))
       
)</errorConditionFormula>
        <errorMessage>Wrong status</errorMessage>
    </validationRules>
